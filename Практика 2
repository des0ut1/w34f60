//практическая работа 2
//1
fun main() {
    val A = true
    val B = false
    val C = false
    val zxc = A || B
    println("а) А или В: $zxc")
    val zxcc = A && B
    println("б) А и В: $zxcc")
    val zxccc = B || C
    println("в) В или С: $zxccc")
}

//практическая работа 2
//2
fun main() {
    val X = false
    val Y = true
    val Z = false
    val zxc = X || Z
    println("а) X или Z: $zxc")
    val zxcc = X && Y
    println("б) X и Y: $zxcc")
    val zxccc = X && Z
    println("в) X и Z: $zxccc")
}

//практическая работа 2
//3
fun main() {
    val A = true
    val B = false
    val C = false
    val zxc = !A && B
    println("а) не А и В: $zxc")
    val zxcc = A || !B
    println("б) А или не В: $zxcc")
    val zxccc = (A && B) || C
    println("в) А и В или С: $zxccc")
}

//практическая работа 2
//4
fun main() {
    val X = true
    val Y = true
    val Z = false
    val zxc = !X && Y
    println("а) не X и Y: $zxc")
    val zxcc = X || !Y
    println("б) X или не Y: $zxcc")
    val zxccc = X || (Y && Z)
    println("в) X или Y и Z: $zxccc")
}

//практическая работа 2
//5
fun main() {
    val X = true
    val Y = true
    val Z = false
    val zxc = !X && Y
    println("а) не X и Y: ${!X && Y}")
    val zxcc = X || !Y
    println("б) X или не Y: ${X || !Y}")
    val zxccc = X || (Y && Z)
    println("в) X или Y и Z: ${X || (Y && Z)}")
}

//практическая работа 2
//6
fun main() {
    val X = false
    val Y = false
    val Z = true
    val zxc = X || (Y && !Z)
    println("а) X или Y и не Z: ${X || (Y && !Z)}")
    val zxcc = X && (!Y || Z)
    println("б) X и не Y или Z: ${X && (!Y || Z)}")
    val zxccc = !X && !Y
    println("в) не X и не Y: ${!X && !Y}")
    val zzxc = X && (!Y || Z)
    println("г) X и (не Y или Z): ${X && (!Y || Z)}")
    val zzzxc = !(X && Z) || Y
    println("д) не (X и Z) или Y: ${!(X && Z) || Y}")
    val zxxc = X || !(Y || Z)
    println("е) X или (не (Y или Z)): ${X || !(Y || Z)}")
}

//практическая работа 2
//7
fun main() {
    val A = true
    val B = false
    val C = false
    val zxc = A || !(A && B) || C
    println("а) А или не (А и В) или С: ${A || !(A && B) || C}")
    val zxcc = !A || A && (B || C)
    println("б) не А или А и (В или С): ${!A || A && (B || C)}")
    val zxccc = (A || B && !C) && C
    println("в) (А или В и не С) и С: ${(A || B && !C) && C}")
}

