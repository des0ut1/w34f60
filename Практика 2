//практическая работа 2
//1
fun main() {
    val z = 47
    val x = z / 10
    val c = z % 10
    val s = x + c
    val d = x * c
    println("Число: $z")
    println("a) Число десятков: $x")
    println("b) Число единиц в нем: $c")
    println("c) Сумма цифр: $s")
    println("d) Произведение его цифр: $d")
}

//практическая работа 2
//2
fun main() {
    val z = 365
    val x = z % 10
    val c = (z / 10) % 10
    val v = z / 100
    val s = x + c + v
    val d = x * c * v
    println("Число: $z")
    println("Число единиц: $x")
    println("Число десятков: $c")
    println("Сумма цифр: $s")
    println("Произведение цифр: $d")
}

//практическая работа 2
//3
fun main() {
    println("Введите первое число:")
    val zxc1 = readLine()!!.toDouble()
    println("Введите второе число:")
    val zxc2 = readLine()!!.toDouble()

    if (zxc2 != 0.0) {
        val z = zxc1 / zxc2
        println("$zxc1 разделенное на $zxc2 равно $z")
    } else {
        println("ОШИБКА!!!ДЕЛЕНИЕ НА НОЛЬ НЕВОЗМОЖНО!:(")
    }
}

//практическая работа 2
//4
fun main() {
    val zxc = 5
    val zxc1 = 3
    var zxc2 = 1
            for (i in 1..zxc1) {
                zxc2 *= zxc
            }
    println("$zxc в степени $zxc1 равно $zxc2")
}

//практическая работа 2
//5
fun main() {
    print("Введите число: ")
    val zxc = readLine()?.toDouble() ?: 0.0
    val zxc1 = Math.sqrt(zxc)
    println("Корень числа $zxc = $zxc1")
}

